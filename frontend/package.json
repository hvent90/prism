{
  "name": "prism-frontend",
  "version": "1.0.0",
  "description": "Frontend for Prism Python code analysis dashboard",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "vite",
    "dev:build": "vite build",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "serve": "vite preview --port 8080",
    "start": "npm run build && npm run serve"
  },
  "dependencies": {
    "monaco-editor": "^0.44.0",
    "d3": "^7.8.5"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0",
    "vite": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/d3": "^7.4.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "typescript",
    "code-analysis",
    "python",
    "dashboard",
    "monaco-editor"
  ],
  "author": "Prism Team",
  "license": "MIT"
}
