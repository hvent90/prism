{
  "name": "prism-frontend",
  "version": "1.0.0",
  "description": "Frontend for Prism Python code analysis dashboard",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 8080",
    "start": "npm run dev",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "monaco-editor": "^0.44.0",
    "@monaco-editor/react": "^4.6.0",
    "d3": "^7.8.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/d3": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.5",
    "vitest": "^1.0.4",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0",
    "vite": "^5.0.0",
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "react",
    "typescript",
    "code-analysis",
    "python",
    "dashboard",
    "monaco-editor"
  ],
  "author": "Prism Team",
  "license": "MIT"
}
